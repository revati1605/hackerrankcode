#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define MAX_LEN 40

int main() 
{

    char ch;
    char s[MAX_LEN];
    char sen[MAX_LEN];
    scanf("%c",&ch);
    scanf("%s",s);
    scanf("\n");
    scanf("%[^\n]%*c",sen);
    printf("%c\n%s\n%s\n",ch,s,sen);
    
     
    return 0;
}
